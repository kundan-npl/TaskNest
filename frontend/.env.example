# Frontend Environment Variables for TaskNest

# API Endpoints
VITE_API_URL=https://api.tasknest.com/api/v1

# AWS S3 Configuration for Frontend Direct Uploads (if needed)
VITE_AWS_BUCKET_NAME=tasknest-files
VITE_AWS_REGION=us-east-1

# Feature Flags
VITE_ENABLE_NOTIFICATIONS=false
VITE_ENABLE_REALTIME=false

# Analytics (if needed for future)
VITE_ANALYTICS_ID=

# Miscellaneous
VITE_MAX_FILE_SIZE=5242880
